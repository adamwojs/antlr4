Lexer(lexer, atn, actionFuncs, sempredFuncs, superClass) ::= <<
use ANTLR\v4\Runtime\ATN\ATN;
use ANTLR\v4\Runtime\ATN\ATNDeserializer;
use ANTLR\v4\Runtime\ATN\LexerATNSimulator;
use ANTLR\v4\Runtime\ATN\PredictionContextCache;
use ANTLR\v4\Runtime\CharStream;
use ANTLR\v4\Runtime\DFA\DFA;
use ANTLR\v4\Runtime\Lexer;
use ANTLR\v4\Runtime\RuleContext;
use ANTLR\v4\Runtime\VocabularyInterface;
use ANTLR\v4\Runtime\Vocabulary;

class <lexer.name> extends <superClass; null="Lexer">
{
	<if(lexer.tokens)>
	public const <lexer.tokens:{k | <k>=<lexer.tokens.(k)>}; separator=", ", wrap, anchor>;

	<endif>
	<if(lexer.channels)>
	public const <lexer.channels:{c | <c>=<lexer.channels.(c)>}; separator=", ", wrap, anchor>;

	<endif>
	<if(rest(lexer.modes))>
	public const <rest(lexer.modes):{m | <m>=<i>}; separator=", ", wrap, anchor>;

	<endif>

	public const CHANNEL_NAMES = [
		"DEFAULT_TOKEN_CHANNEL", "HIDDEN"<if (lexer.channels)>, <lexer.channels:{c| "<c>"}; separator=", ", wrap, anchor><endif>
	];

	public const MODE_NAMES = [
		<lexer.modes:{m| "<m>"}; separator=", ", wrap, anchor>
	];

	public const RULE_NAMES = [
		<lexer.ruleNames:{r | "<r>"}; separator=", ", wrap, anchor>
	];

	<atn>

	<namedActions.members>

	public function __construct(CharStream $input)
	{
		parent::__construct($input);

		$this->_interp = new LexerATNSimulator(
			$this, $this->getATN(), $this->getDecisionToDFA(), new PredictionContextCache()
		);
	}

	public function getGrammarFileName(): string
	{
		return "<lexer.grammarFileName>";
	}

	public function getRuleNames(): array
	{
		return self::RULE_NAMES;
	}

	public function getSerializedATN(): ?array
	{
		return self::SERIALIZED_ATN;
	}

	public function getChannelNames(): array
	{
		return self::CHANNEL_NAMES;
	}

	public function getModeNames(): array
	{
		return self::MODE_NAMES;
	}

	public function getATN(): ATN
	{
		static $atn = null;
		if ($atn === null) {
            $atn = (new ATNDeserializer())->deserialize(self::SERIALIZED_ATN);
        }

		return $atn;
	}

	private function getDecisionToDFA(): array
    {
        static $decisionToDFA = null;

        if ($decisionToDFA === null) {
            $atn = $this->getATN();

            $decisionToDFA = [];
            for ($i = 0; $i \< $atn->getNumberOfDecisions(); $i++) {
                $decisionToDFA[] = new DFA($atn->getDecisionState($i), $i);
            }
        }

        return $decisionToDFA;
    }

	<vocabulary(lexer.literalNames, lexer.symbolicNames)>
	<dumpActions(lexer, "", actionFuncs, sempredFuncs)>
}
>>

SerializedATN(model) ::= <<
<! only one segment, can be inlined !>
public const SERIALIZED_ATN = [
	<model.serialized; wrap={<\n>}>
];
>>

