ContextTokenGetterDecl(t) ::= <<
public function <t.name>(): TerminalNode
{
	return $this->getToken(<parser.name>::<t.name>, 0);
}

>>

// Should never be called
ContextTokenListGetterDecl(t) ::= <<
public function <t.name>List(): array
{
	return $this->getTokens(<parser.name>::<t.name>);
}

>>

ContextTokenListIndexedGetterDecl(t) ::= <<
/**
 * @param int|null $i
 *
 * @return \ANTLR\v4\Runtime\Tree\TerminalNode|\ANTLR\v4\Runtime\Tree\TerminalNode[]
 */
public function <t.name>(?int $i = null)
{
	if ($i === null) {
		return $this->getTokens(<parser.name>::<t.name>);
	}

	return $this->getToken(<parser.name>::<t.name>, $i);
}

>>

ContextRuleGetterDecl(r) ::= <<
public function <r.name>(): <r.ctxName>
{
	return $this->getRuleContext(<r.ctxName>::class, 0);
}

>>

// Should never be called
ContextRuleListGetterDecl(r) ::= <<
public function <r.name>List(): array
{
	return $this->getRuleContexts(<r.ctxName>::class);
}

>>

ContextRuleListIndexedGetterDecl(r) ::= <<
/**
 * @param int|null $i
 *
 * @return <r.ctxName>|<r.ctxName>[]
 */
public function <r.name>(?int $i = null)
{
	if ($i === null) {
		return $this->getRuleContexts(<r.ctxName>::class);
	}

	return $this->getRuleContext(<r.ctxName>::class, $i);
}

